	number = {3},
	urldate = {2024-11-21},
	journal = {Nutrients},
	author = {Ziani, Khaled and Ioniță-Mîndrican, Corina-Bianca and Mititelu, Magdalena and Sorinel, Marius Neacșu and Negrei, Carolina and Moroșan, Elena and Drăgănescu, Doina and Preda, Olivia-Teodora},
