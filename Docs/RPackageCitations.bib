	keywords = {Proteins, Bacteria, Antibiotics, Viruses, Biomolecules, Antigens, Pathogens, Plastic pollution, Immune system, Microplastics, Allergens, bacteria, Biofilms, Chemical composition, Digestive system, host–pathogen interaction, Immune response, Inflammation, Inflammatory response, innate immunity, Microorganisms, microplastics, Pathogen, Pattern recognition, Phagocytosis, Seawater, Severe acute respiratory syndrome coronavirus 2, Skin, Structure-function relationships, Surface charge, Surfactants, viruses},
	pages = {4065},
